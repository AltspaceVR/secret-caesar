<!DOCTYPE html>
<html>
	<head>
		{{#hitler}}
			<title>Secret Hitler</title>
		{{/hitler}}
		{{#caesar}}
			<title>Secret Caesar</title>
			<link rel='shortcut icon' type='image/png' href='{{basePath}}/static/img/caesar/icon.png'/>
			<link rel='icon' type='image/png' href='{{basePath}}/static/img/caesar/icon.png'/>
		{{/caesar}}
		{{^localLibs}}
			<script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/86/three.min.js"></script>
			<script src="https://cdn.rawgit.com/mrdoob/three.js/r84/examples/js/loaders/ColladaLoader.js"></script>
			<script src="https://cdn.rawgit.com/mrdoob/three.js/0548b13/examples/js/loaders/GLTFLoader.js"></script>
			<script src="https://sdk.altvr.com/libs/altspace.js/2.4.5/altspace.min.js"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/tween.js/17.1.0/Tween.min.js"></script>
			<script src="https://cdn.rawgit.com/stevenvergenz/altspace-diorama/970a38b/dist/diorama.min.js"></script>
		{{/localLibs}}
		{{#localLibs}}
			<script src='{{basePath}}/static/lib/three.r86.js'></script>
			<script src='{{basePath}}/static/lib/ColladaLoader.r84.js'></script>
			<script src='{{basePath}}/static/lib/GLTFLoader.0548b13.js'></script>
			<script src="{{basePath}}/static/lib/altspace.245.js"></script>
			<script src="{{basePath}}/static/lib/Tween.17.1.0.js"></script>
			<script src='{{basePath}}/static/lib/diorama.970a38b.js'></script>
		{{/localLibs}}
		<script src="{{basePath}}/static/socket.io/socket.io.min.js"></script>
		{{#sentry_client}}
		<script src="https://cdn.ravenjs.com/3.20.1/raven.min.js" crossorigin="anonymous"></script>
		<script>Raven.config("{{&sentry_client}}").install();</script>
		{{/sentry_client}}
		<script src='{{basePath}}/static/js/secrethitler.js'></script>
	</head>
	<body>
		<script>
		{{#sentry_client}}
			Raven.context(function(){
				window.d = new Diorama({gridOffset: [1.5, -1.5, 1.5]});
				d.start(SecretHitler);
			});
		{{/sentry_client}}
		{{^sentry_client}}
			window.d = new Diorama({gridOffset: [1.5, -1.5, 1.5]});
			d.start(SecretHitler);
		{{/sentry_client}}
		</script>
	</body>
</html>
